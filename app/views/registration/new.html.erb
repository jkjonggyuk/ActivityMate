<%= form_with scope: :user, url: users_path, local: true do |form| %>

<h1> Registration </h1>




<p>
    <%= form.label :Name %><br>
    <%= form.text_field :name %>
  </p>

  <p>
    <%= form.label :Email %><br>
    <%= form.text_field :email %>
  </p>
 
   <p>
    <%= form.label :Email_confirmation %><br>
    <%= form.text_field :email_confirmation %>
  </p>

  <p>
    <%= form.label :Password %><br>
    <%= form.text_field :password %>
  </p>
 
  <p>
    <%= form.submit "Submit" %> 
  </p>
<% end %>


